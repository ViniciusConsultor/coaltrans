<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
   assembly="NHSample.Domain"
   namespace="NHSample.Domain.Entities">

	<class name ="GongdanMailInfo"  table="gongdan_mail_info" lazy="true">
		<id name="Id" column ="Id">
			<generator class ="native"/>
		</id>
		<property name ="Gongdan_id"/>
		<property name ="Msg_sender"/>
		<property name ="Msg_to"/>
		<property name ="Msg_title"/>
		<property name ="Msg_body"/>
		<property name ="Status"/>
		<property name ="Creation_date"/>
		<property name ="Send_date"/>
	</class>
</hibernate-mapping>